<!DOCTYPE html>
<html class="no-js">
<head>
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width" />
	<title>@ViewBag.Title</title>
	@Styles.Render("~/Static/Resources/reset/css")
	@Styles.Render("~/Static/Resources/jquery-ui/css")
	@Styles.Render("~/Static/Resources/jquery.layout/css")
	@Styles.Render("~/Static/Resources/uploadify/css")
	@Styles.Render("~/Static/Resources/site/css")

	@Scripts.Render("~/Static/Resources/modernizr/js")
	@Scripts.Render("~/Static/Resources/jquery/js")
	@Scripts.Render("~/Static/Resources/jquery-ui/js")
	@Scripts.Render("~/Static/Resources/jquery.layout/js")
	@Scripts.Render("~/Static/Resources/json2/js")
	@Scripts.Render("~/Static/Resources/uploadify/js")

	@Scripts.Render("~/Static/extendjs/js")

</head>
<body>
	<header>
		<div class="body main_header">
			<div class="logo_150x40">
			</div>
			<nav class="main_nav">
				<ul>
					<li><a href="javascript:void(0);">登陆</a></li>
					<li><a href="javascript:void(0);">我的订单</a></li>
					<li><a href="javascript:void(0);">我的设计</a></li>
				</ul>
			</nav>
		</div>
	</header>
	<div class="body">
		@RenderBody()
	</div>
	@RenderSection("scripts", required: false)
</body>
</html>
